<h1>Add a new material!</h1>

<%= form_for(@material) do |f| %>
  <div class="field">
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </div>
  
  <div class="text">
  	<%= f.label :description%><br />
  	<%= f.text_area :description %>
  </div>
  
  <div class="actions">
  	<%= f.submit %>
  </div>

<% end %>
